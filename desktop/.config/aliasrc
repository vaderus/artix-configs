alias p="doas pacman -S"
alias pr="doas pacman -Rns"
alias ps="pacman -Ss"
alias update="doas pacman -Syyu"
alias P="paru -S"
alias Pr="paru -Rns"
alias Ps="paru -Ss"
alias Pc="paru -c"
alias makest="doas make clean install -C /home/vader/.sources/st"
alias makedmenu="doas make clean install -C /home/vader/.sources/dmenu"
alias quantas="pacman -Q | wc -l"
alias freedamem="sync && doas sh -c 'echo 3 > /proc/sys/vm/drop_caches'"
